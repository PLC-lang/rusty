target triple = "[filtered]"

@llvm.global_ctors = appending global [1 x { i32, ptr, ptr }] [{ i32, ptr, ptr } { i32 0, ptr @__init___plc, ptr null }]

define i32 @main() {
entry:
  %main = alloca i32, align 4
  %x = alloca i8, align 1
  %a = alloca i32, align 4
  store i8 0, ptr %x, align 1
  store i32 0, ptr %a, align 4
  store i32 0, ptr %main, align 4
  store i8 0, ptr %x, align 1
  %load_x = load i8, ptr %x, align 1
  br i8 %load_x, label %lbl, label %else_block

else_block:                                       ; preds = %entry
  store i32 2, ptr %a, align 4
  br label %lbl

lbl:                                              ; preds = %else_block, %entry
  %load_a = load i32, ptr %a, align 4
  %tmpVar = add i32 %load_a, 3
  store i32 %tmpVar, ptr %a, align 4
  %load_a1 = load i32, ptr %a, align 4
  store i32 %load_a1, ptr %main, align 4
  %main_ret = load i32, ptr %main, align 4
  ret i32 %main_ret
}

define void @__init___plc() {
entry:
  ret void
}
