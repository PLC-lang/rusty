(**
 * @}
 * @addtogroup ErrorReason
 * @brief Error reasons codes.
 *
 * The error reason ranges are:
 *  - [   1 -  999] system errors (e.g. errno)
 *  - [1000 - 1999] general errors
 *  - [2000 - 4095] custom error (additional library specific errors)
 * @{
 *)
VAR_GLOBAL CONSTANT
    ERROR_RANGE_SYSTEM_MIN : DINT := 1;                     (**< @brief Minimum value for system error codes *)
    ERROR_RANGE_SYSTEM_MAX : DINT := 999;                   (**< @brief Maximum value for system error codes *)
