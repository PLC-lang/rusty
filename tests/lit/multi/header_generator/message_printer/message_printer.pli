TYPE T_Message : STRING[255];
END_TYPE

FUNCTION PrintMessage
VAR_INPUT
    message: T_Message;
END_VAR
END_FUNCTION

FUNCTION ManualMultiMessagePrinter
VAR_INPUT
    count: INT;
    messages: T_Message...;
END_VAR
END_FUNCTION

FUNCTION SizedMultiMessagePrinter
VAR_INPUT
    messages: {sized} T_Message...;
END_VAR
END_FUNCTION
