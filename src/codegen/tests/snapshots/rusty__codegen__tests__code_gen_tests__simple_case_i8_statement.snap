%prg = type { i8, i8 }

@prg_instance = global %prg zeroinitializer, section "var-prg_instance:r2u8u8"

define void @prg(%prg* %0) section "fn-prg:v" {
entry:
  %x = getelementptr inbounds %prg, %prg* %0, i32 0, i32 0
  %y = getelementptr inbounds %prg, %prg* %0, i32 0, i32 1
  %load_x = load i8, i8* %x, align 1
  switch i8 %load_x, label %else [
    i8 1, label %case
    i8 2, label %case1
    i8 3, label %case2
  ]

case:                                             ; preds = %entry
  store i8 1, i8* %y, align 1
  br label %continue

case1:                                            ; preds = %entry
