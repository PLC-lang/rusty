define void @prg(%prg* %0) section "fn-prg:v" {
entry:
  %x = getelementptr inbounds %prg, %prg* %0, i32 0, i32 0
  %y = getelementptr inbounds %prg, %prg* %0, i32 0, i32 1
  %load_x = load i32, i32* %x, align 4
  switch i32 %load_x, label %else [
    i32 1, label %case
