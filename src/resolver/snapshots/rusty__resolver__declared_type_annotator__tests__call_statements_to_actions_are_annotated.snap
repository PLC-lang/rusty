    fb.fbAction [<FB.fbAction (Program)>]
        fb [<FB (Variable TEST.fb)>]
    a := 1, b := TRUE, c := 7 [<->]
        a := 1 [<->]
            a [<INT (Variable FB.a)>]
            1 [<DINT>]
        b := TRUE [<->]
