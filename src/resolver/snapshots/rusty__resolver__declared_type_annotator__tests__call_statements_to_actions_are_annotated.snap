prg.prgAction(a := 1, b := TRUE, c := 7) [<->]
    prg.prgAction [<PRG.prgAction (Program)>]
        prg [<PRG (Program)>]
    a := 1, b := TRUE, c := 7 [<->]
        a := 1 [<->]
            a [<INT (Variable PRG.a)>]
            1 [<DINT>]
        b := TRUE [<->]
            b [<BOOL (Variable PRG.b)>]
            TRUE [<BOOL>]
        c := 7 [<->]
            c [<BYTE (Variable PRG.c)>]
            7 [<DINT>]
