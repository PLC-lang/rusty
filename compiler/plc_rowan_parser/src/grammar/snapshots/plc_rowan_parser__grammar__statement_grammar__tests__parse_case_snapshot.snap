BODY
  EXPRESSION_STMT
    CASE_STATEMENT
      CASE_KW "CASE"
      EXPRESSION_STMT
        NAME_REF
          IDENT "iCondition"
      OF_KW "OF"
      CASE_ARM
        EXPRESSION_LIST
          LITERAL
            INT_NUMBER "1"
          COMMA ","
          LITERAL
            INT_NUMBER "5"
        COLON ":"
        BODY
          EXPRESSION_STMT
            ASSIGNMENT
              NAME
                IDENT "bVar1"
              ASSIGN ":="
              EXPRESSION_STMT
                LITERAL
                  INT_NUMBER "1"
            SEMICOLON ";"
      CASE_ARM
        EXPRESSION_LIST
          LITERAL
            INT_NUMBER "2"
        COLON ":"
        BODY
          EXPRESSION_STMT
            ASSIGNMENT
              NAME
                IDENT "bVar2"
              ASSIGN ":="
              EXPRESSION_STMT
                LITERAL
                  INT_NUMBER "0"
            SEMICOLON ";"
      END_CASE_KW "END_CASE"
